<serviceGroup>
    <service name="VOPService">
        <messageReceivers>
            <messageReceiver mep="http://www.w3.org/ns/wsdl/in-out" class="com.tydic.commun.mng.VOPServiceMessageReceiverInOut"/>
        </messageReceivers>
        <parameter name="ServiceClass">com.tydic.commun.mng.VOPServiceSkeleton</parameter>
        <parameter name="useOriginalwsdl">true</parameter>
        <parameter name="modifyUserWSDLPortAddress">true</parameter>
        <operation name="service" mep="http://www.w3.org/ns/wsdl/in-out" namespace="http://mng.commun.tydic.com">
            <actionMapping>http://mng.commun.tydic.com/service</actionMapping>
            <outputActionMapping>http://mng.commun.tydic.com/VOPService/serviceResponse</outputActionMapping>
        </operation>
    </service>
</serviceGroup>